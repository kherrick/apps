{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "apps": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "x",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/apps",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "encoding",
              "hammerjs",
              "long",
              "node-forge",
              "pwgen",
              "sdp",
              "seedrandom",
              "whatwg-url"
            ],
            "assets": [
              "src/favicon.ico",
              "src/404.html",
              "src/assets",
              "src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "./node_modules/minimal-c-sharp-wasm/dist",
                "output": "./assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@tensorflow/tfjs-backend-wasm/dist",
                "output": "./assets/tfjs-backend-wasm/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/onnxruntime-web/dist",
                "output": "./assets/onnxruntime-web/"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "server": "src/main.server.ts",
            "prerender": {
              "routesFile": "src/app/routes.list.txt",
              "discoverRoutes": true
            },
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1MB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "30kB",
                  "maximumError": "100kB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "apps:build:production"
            },
            "development": {
              "buildTarget": "apps:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/404.html",
              "src/assets",
              "src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "./node_modules/minimal-c-sharp-wasm/dist",
                "output": "./assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@tensorflow/tfjs-backend-wasm/dist",
                "output": "./assets/tfjs-backend-wasm/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/onnxruntime-web/dist",
                "output": "./assets/onnxruntime-web/"
              }
            ],
            "styles": [
              "src/styles.scss"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
